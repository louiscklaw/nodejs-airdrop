{
  "name": "airdropjs",
  "version": "0.9.5",
  "description": "A simple airdrop like application, run the nodejs server and you can transfer files accross any device",
  "scripts": {
    "start": "node airdrop.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,html,jsx,tsx,ts,json}\"",
    "gitUpdate": "git add . && git commit -m \"update nodejs-airdrop,\" && git push",
    "docker_rebuild": "docker compose kill && docker compose down && docker compose up -d",
    "into_docker": "docker compose exec -it airdrip bash"
  },
  "keywords": [
    "nodejs",
    "airdrop",
    "file",
    "transfer",
    "fileshare",
    "toy"
  ],
  "author": {
    "name": "Conner Maddalozzo",
    "url": "http://github.com/lozzoc"
  },
  "bin": {
    "nodemon": "./airdrop.js"
  },
  "license": "ISC",
  "dependencies": {
    "address": "^1.1.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-handlebars": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
